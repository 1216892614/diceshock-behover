{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "strict": true,
        "skipLibCheck": true,
        "lib": ["ESNext", "dom"],
        "types": ["vite/client", "vite-plugin-svgr/client"],
        "jsx": "react-jsx",
